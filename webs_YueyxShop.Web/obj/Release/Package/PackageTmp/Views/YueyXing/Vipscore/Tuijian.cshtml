@using webs_YueyxShop.Model

@{
    Layout = null;
}

@foreach (var i in Model)
{ 
    <li>
        <dl>
            <dt><a href="/ProDetail/ProDetail?skuid=@i.productSkuInfo.sku_ID"><img name="page_cnt_1" _src="@i.productImgInfo.pi_Url" alt="@i.p_Name" /></a></dt>
            <dd><a href="/ProDetail/ProDetail?skuid=@i.productSkuInfo.sku_ID">@i.p_Name</a></dd>
            <dd><span>￥@i.productSkuInfo.sku_Price</span><del>￥@i.productSkuInfo.sku_scPric</del></dd>
        </dl>
    </li>
    
}