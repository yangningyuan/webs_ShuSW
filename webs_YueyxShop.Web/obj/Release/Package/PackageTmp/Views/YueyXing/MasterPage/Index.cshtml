            $(function () {
                countDown("2014/9/24 23:59:59", "#time01 .day", "#time01 .hour", "#time01 .minute", "#time01 .second");
                countDown("2014/10/10 22:59:59", "#time02 .day", "#time02 .hour", "#time02 .minute", "#time02 .second");
                countDown("2014/11/10 22:59:59", "#time03 .day", "#time03 .hour", "#time03 .minute", "#time03 .second");
                countDown("2014/12/24 23:59:59", "#time04 .day", "#time04 .hour", "#time04 .minute", "#time04 .second");
                countDown("2014/12/10 22:59:59", "#time05 .day", "#time05 .hour", "#time05 .minute", "#time05 .second");
                countDown("2014/12/20 22:59:59", "#time06 .day", "#time06 .hour", "#time06 .minute", "#time06 .second");
                countDown("2015/1/24 23:59:59", "#time07 .day", "#time07 .hour", "#time07 .minute", "#time07 .second");
                countDown("2015/2/10 22:59:59", "#time08 .day", "#time08 .hour", "#time08 .minute", "#time08 .second");
                countDown("2015/3/10 22:59:59", "#time09 .day", "#time09 .hour", "#time09 .minute", "#time09 .second");
            });

            function countDown(time, day_elem, hour_elem, minute_elem, second_elem) {
                //if(typeof end_time == "string")
                var end_time = new Date(time).getTime(), //月份是实际月份-1
                //current_time = new Date().getTime(),
	        sys_second = (end_time - new Date().getTime()) / 1000;
                var timer = setInterval(function () {
                    if (sys_second > 0) {
                        sys_second -= 1;
                        var day = Math.floor((sys_second / 3600) / 24);
                        var hour = Math.floor((sys_second / 3600) % 24);
                        var minute = Math.floor((sys_second / 60) % 60);
                        var second = Math.floor(sys_second % 60);
                        day_elem && $(day_elem).text(day); //计算天
                        $(hour_elem).text(hour < 10 ? "0" + hour : hour); //计算小时
                        $(minute_elem).text(minute < 10 ? "0" + minute : minute); //计算分
                        $(second_elem).text(second < 10 ? "0" + second : second); // 计算秒
                    } else {
                        clearInterval(timer);
                    }
                }, 1000);
            }
