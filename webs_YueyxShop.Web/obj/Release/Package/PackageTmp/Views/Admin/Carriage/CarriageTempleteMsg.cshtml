@using System.Collections.Generic
@using System.Data
<!DOCTYPE html>
<html>
<head>
    <title>月月兴食品商行后台管理系统</title>
    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            divSearch($("#pagerForm", navTab.getCurrentPanel()), "carrtBox");
        });
    </script>
</head>
<body>
<form id="pagerForm" method="post"  action="@Url.Action("CarriageTempleteList", "Carriage")" onsubmit="return divSearch(this,'carrtBox');">
    <input type="hidden" id="hfPid" name="hfPid" value="0" />
    <input type="hidden" id="at_add" name="at_add" />
    <input type="hidden" id="hf_daohang" name="hf_daohang" />
    <input type="hidden" id="hf_cengj" name="hf_cengj" value="1" />
    <input type="hidden" id="hf_bianma" name="hf_bianma" />
    <input type="hidden" id="hf_type" name="type" />
    <div class="pageHeader">
        <div class="searchBar">
            <table class="searchContent">
                <tr>
                    <td >
                    </td>
                    <td>
                    </td>
                    <td id="td_name">
                        名称：
                    </td>
                    <td>
                        <input type="text" name="txtctname" />
                    </td>
                    <td id="td_type">
                        计价方式：
                    </td>
                    <td>
                       @Html.DropDownList("select1")
                    </td>
                </tr>
            </table>
            <div class="subBar">
                <ul>
                    <li>
                        <div class="buttonActive">
                            <div class="buttonContent">
                                <button type="submit" id="btn_Select">
                                    检索</button></div>
                        </div>
                    </li>
                    <li>
                        <div class="buttonActive">
                            <div class="buttonContent">
                                <button type="reset" id="reset" >
                                    重置</button></div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
        </div>
        <!--列表-->
        <div class="pageContent">
        <div class="panelBar">
            <ul class="toolBar">
                <li id="ctlayer"><a class="details" href="@Url.Action("CarriageMsg?dli_id={ct_id}", "Carriage")"  target="navTab" ><span>查看详情</span></a></li>
                <li id="ctline" class="line">line</li>
                @if (Convert.ToBoolean(ViewData["IsHasFunRole"]))
                {
                    <li><a class="add" href="@Url.Action("CarriageTempleteEdit?otype=add", "Carriage")"  target="dialog" rel="w_CarriageTempleteEdit">
                        <span>添加</span></a></li>
                    <li class="line">line</li>
                    <li><a class="edit" href="@Url.Action("CarriageTempleteEdit?otype=modify&dli_id={ct_id}", "Carriage")" rel="w_CarriageTempleteEdit"
                        target="dialog"  ><span>编辑</span></a></li>
                    <li class="line">line</li>
                    <li><a class="delete" href="@Url.Action("CarriageTempleteDelete?dli_id={ct_id}", "Carriage")" rel="w_CarriageTempleteDelete"
                    target="ajaxTodo" title='确定删除' warn="请选择信息！！！"><span>删除</span></a></li>
                    <li class="line">line</li>
                    <li><a class="unlock"  href="@Url.Action("CarriageTempleteEnable?otype=enable&dli_id={ct_id}", "Carriage")"
                    target="ajaxTodo" warn="请选择信息！！！"><span>启用</span></a></li>
                    <li><a class="lock"  href="@Url.Action("CarriageTempleteEnable?otype=disable&dli_id={ct_id}", "Carriage")"
                    target="ajaxTodo" warn="请选择信息！！！"><span>禁用</span></a></li>
                }
            </ul>
        </div>
        <div id="carrtBox">

        </div>
    </div>
</form>
</body>
</html>
