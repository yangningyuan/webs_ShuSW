@model webs_YueyxShop.Model.OrderBase
<!DOCTYPE html>
<html>
<head>
    <title>月月兴食品商行后台管理系统</title>
</head>
<body>
       <form id="form1" runat="server" method="post" action="@Url.Action("OrderBaseEdit", "OrderBase")"  enctype="multipart/form-data"
       class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
    @Html.HiddenFor(model => model.o_ID)
    @Html.Hidden("texOtype", @ViewData["texOtype"])
    @Html.Hidden("hfPId", @ViewData["hfPId"])
    <div class="pageContent">
        <div class="pageFormContent" layouth="52">
            <div class="unit">
                <label>
                    订单编号：</label>
                    @Html.TextBoxFor(model => model.o_Code, new { @class = "required", @readonly="readonly", @style = "width:200px;" })
            </div>
            <div class="unit" >
                <label>
                    物流单号：</label>
                    @Html.TextBoxFor(model => model.o_LogisticsNo, new { @class = "required", minlength = "4", maxlength = "22", @style = "width:200px;" })
            </div>
        </div>
        <div class="formBar">
            <ul>
                @if (Convert.ToBoolean(ViewData["IsHasFunRole"]))
                {
                    <li id="save">
                        <div class="buttonActive">
                            <div class="buttonContent">
                                <button type="submit" >
                                    保存</button></div>
                        </div>
                    </li>
                }
                <li>
                    <div class="button">
                        <div class="buttonContent">
                            <button type="button" class="close">
                                关闭</button></div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    </form>
</body>
</html>
