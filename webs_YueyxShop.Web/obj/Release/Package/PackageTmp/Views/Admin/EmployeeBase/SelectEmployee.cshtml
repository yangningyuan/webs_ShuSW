@using System.Collections.Generic
@using System.Data
<!DOCTYPE html>
<html>
<head id="Head1">
    <title>月月兴食品商行后台管理系统</title>
    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            divSearch($("#pagerForm", $.pdialog.getCurrent()), "SelectEmployeeBox");
        });

        function SelectEmploy_SelValueNone() {
            $.bringBack({ id: '', orgName: '', orgNum: '' })
        }
    </script>
</head>
<body>
    <form id="pagerForm" method="post" action="@Url.Action("SelectEmployeeList", "EmployeeBase")" onsubmit="return divSearch(this,'SelectEmployeeBox');">
    @Html.Hidden("RoleLevelId", @ViewData["RoleLevelId"])
     @Html.Hidden("d_ID", @ViewData["d_ID"])
    <div class="pageHeader">
        <div class="searchBar">
            <table class="searchContent">
                <tr>
                    <td>
                        部门名称：
                    </td>
                    <td>
                        @Html.TextBox("depName")
                    </td>
                    <td>
                        用户姓名 ：
                    </td>
                    <td>
                        @Html.TextBox("employeeName")
                    </td>
                    <td>
                        工号 ：
                    </td>
                    <td>
                        @Html.TextBox("GongHao")
                    </td>
                </tr>
            </table>
            <div class="subBar">
                <ul>
                    <li>
                        <div class="buttonActive">
                            <div class="buttonContent">
                                <button type="submit">
                                    检索</button></div>
                        </div>
                    </li>
                    <li>
                        <div class="buttonActive">
                            <div class="buttonContent">
                                <button type="button" onclick="SelectEmploy_SelValueNone();">
                                    取消</button></div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <!--列表-->
    <div class="pageContent">
        @*        <div class="panelBar">
        </div>*@
        <div id="SelectEmployeeBox">
        </div>
    </div>
    </form>
</body>
</html>
