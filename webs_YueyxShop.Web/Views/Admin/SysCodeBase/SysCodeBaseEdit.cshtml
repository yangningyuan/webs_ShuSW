@model webs_YueyxShop.Model.SysCodeBase
<!DOCTYPE html>
<html>
<head id="Head1">
    <title>月月兴食品商行后台管理系统</title>
</head>
<body>
    <form id="form1" runat="server" method="post" action="@Url.Action("SysCodeBaseEdit", "SysCodeBase")"
    class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
    @Html.HiddenFor(model => model.sc_ID)
    @Html.HiddenFor(model => model.sc_LeiB)
    @Html.HiddenFor(model => model.sc_ParentId)
    @Html.HiddenFor(model => model.sc_BianM)
    @Html.HiddenFor(model => model.sc_CengJ)
    @Html.HiddenFor(model => model.sc_CreatedBy)
    @Html.HiddenFor(model => model.sc_CreatedOn)
    @Html.HiddenFor(model => model.sc_DeleteStateCode)
    @Html.HiddenFor(model => model.sc_StateCode)
    @Html.Hidden("texOtype", @ViewData["texOtype"])
    @Html.Hidden("pid", @ViewData["pid"])
    @Html.Hidden("hfcengj", @ViewData["hfcengj"])
    @Html.Hidden("hfbianm", @ViewData["hfbianm"])
    <div class="pageContent">
        <div class="pageFormContent" layouth="58">
            <dl>
                <dt>名称：</dt>
                <dd>@Html.TextBoxFor(model => model.sc_MingCh, new { @class = "required", @size = "30" })</dd>
            </dl>
        </div>
        <div class="formBar">
            <ul>
                @if (Convert.ToBoolean(ViewData["IsHasFunRole"]))
                {
                    <li>
                        <div class="buttonActive">
                            <div class="buttonContent">
                                <button type="submit">
                                    保存</button></div>
                        </div>
                    </li>
                }
                <li>
                    <div class="button">
                        <div class="buttonContent">
                            <button type="button" class="close">
                                取消</button></div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    </form>
</body>
</html>
