    <div class="side-cont alignright">
      <div class="pro-cont"> 
          <ul class="company-list">
          @{
              if (ViewData["companyEnter"] != null)
                {
                    foreach (var member in (List<webs_YueyxShop.Model.MemberBase>)ViewData["companyEnter"])
                    {
                        <li>
              	          <div class="companys">
                  	          <div class="company-logo"><img style="width:180px;height:110px;" name="page_cnt_1" src="@member.m_HeadImg" alt="商家LOGO" /></div>
