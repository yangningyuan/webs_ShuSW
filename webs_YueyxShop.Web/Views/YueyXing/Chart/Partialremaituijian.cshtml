@using webs_YueyxShop.Model

@{
    Layout = null;
}
@foreach (var i in Model)
{
    <li>
        <div class="pro-img"><a href="/ProDetail/ProDetail?skuid=@i.productSkuInfo.sku_ID"><img name="page_cnt_1" _src="@i.productImgInfo.pi_Url" alt="@i.p_Name" /></a></div>
        <p><a href="/ProDetail/ProDetail?skuid=@i.productSkuInfo.sku_ID">@i.p_Name @i.productAttrInfo.pa_Name</a></p>
        <p><span>￥@i.productSkuInfo.sku_Price</span><del>￥@i.productSkuInfo.sku_scPric</del></p>
    </li>
}