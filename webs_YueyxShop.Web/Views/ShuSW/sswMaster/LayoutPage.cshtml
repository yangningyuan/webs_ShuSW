    <script type="text/javascript">
        function tuichu() {
            $.ajax({
                url: "/_ashx/dataSave_manager_json.ashx?type=removecookie",
                type: "POST",
                data: "",
                async: false,
                error: function (request) {
                    alert("regist error");
                },
                success: function (data) {
                    window.location = "/sswHome/Index";
                    data = strToJson(data);
                    alert(data);
                }
            });
        }
